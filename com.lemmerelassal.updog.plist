<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>com.lemmerelassal.updog</string>

    <key>RunAtLoad</key>
    <true />

    <key>KeepAlive</key>
    <true />

    <key>ProgramArguments</key>
    <array>
      <string>/usr/local/bin/updog</string>
    </array>

    <key>StandardOutPath</key>
    <string>/tmp/updog.log</string>

    <key>StandardErrorPath</key>
    <string>/tmp/updog.error.log</string>
  </dict>
</plist>